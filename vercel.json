{
    "version": 2,
    "builds": [
      {
        "src": "game_server/game_server/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      },
      {
        "src": "game_server/game_server/asgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "game_server/game_server/wsgi.py"
      },
      {
        "src": "/(.*)",
        "dest": "game_server/game_server/asgi.py"
      }
    ],
    "env": {
      "VERCEL_ENV": "development"
    }
  }
  